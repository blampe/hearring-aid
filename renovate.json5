{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  packageRules: [
    {
      matchCategories: [
        'docker',
      ],
      enabled: true,
    },
    {
      matchDatasources: [
        'docker',
      ],
      matchPackageNames: [
        'linuxserver/lidarr',
        'linuxserver-labs/prarr',
      ],
      autoApprove: true,
      automerge: true,
      platformAutomerge: true,
    },
  ],
}
