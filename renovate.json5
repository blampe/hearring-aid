{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  extends: [
    'config:recommended',
  ],
  packageRules: [
    {
      matchDatasources: ['docker'],
      autoApprove: true,
      automerge: true,
    },
    {
      matchDatasources: ["docker"],
      matchPackageNames: ["linuxserver-labs/prarr"],
      versioning: "regex:^lidarr-plugins-(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)\\.(?<build>.+)$",
      autoApprove: true,
      automerge: true,
    },
  ],
}
